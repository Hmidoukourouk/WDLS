{
  "name": "wdls",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder",
    "build-dev": "electron-builder --dir"
  },
  "keywords": [],
  "author": "Expert",
  "license": "ISC",
  "description": "wdls",
  "devDependencies": {
    "electron": "^33.0.2",
